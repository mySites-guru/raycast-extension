{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "mysites-guru",
  "title": "mySites.guru",
  "description": "Search and manage your mySites.guru connected sites",
  "icon": "icon.png",
  "author": "PhilETaylor",
  "license": "MIT",
  "categories": ["Productivity", "Developer Tools"],
  "commands": [
    {
      "name": "search-sites",
      "title": "mySites.guru Site Search",
      "description": "Search your mySites.guru sites and open the management page",
      "mode": "view",
      "keywords": [
        "my",
        "mysites",
        "guru",
        "site",
        "manage"
      ]
    }
  ],
  "preferences": [
    {
      "name": "token",
      "title": "API Token",
      "description": "Your mySites.guru token from https://manage.mysites.guru/en/sites/screenshots",
      "type": "password",
      "required": true
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.93.3",
    "@raycast/utils": "^1.19.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.11",
    "@types/node": "22.13.4",
    "@types/react": "19.0.8",
    "eslint": "^8.57.1",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3"
  },
  "scripts": {
    "build": "ray build",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "prepublishOnly": "echo \"Error: no publish\" && exit 1",
    "publish": "npx @raycast/api@latest publish"
  }
}
